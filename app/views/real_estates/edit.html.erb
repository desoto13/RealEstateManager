<div class="mx-auto border border-info rounded p-3" style="width: 50%;background-color: #e3f2fd;">
  <h1 class="text-center mb-5 mt-3">Real Estate Manager</h1>
  <h2 class="text-center mb-3">Update your <%= @real_estate.type %></h1>
  <%= render "form", real_estate: @real_estate %>

  <%= link_to "Delete", real_estate_path(@real_estate), :method => :delete, class: "btn btn-danger mt-2 btn-sm" %>
</div>